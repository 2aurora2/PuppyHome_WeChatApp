page {
  height: 100%;
}

.container-all {
  width: 100%;
  height: 100%;
}

.bg-img {
  z-index: -1;
  width: 100%;
  height: 100%;
  display: block;
  opacity: 0.5;
}

.return {
  width: 70rpx;
  height: 60rpx;
  position: absolute;
  top: 6%;
  left: 2%;
}

.upLoadImage {
  position: absolute;
  left: 50%;
  bottom: 10%;
  transform: translate(-50%, -50%);
  /* 50%为自身尺寸的一半 */
  -webkit-transform: translate(-50%, -50%);
  border: 5rpx gainsboro solid;
  border-radius: 57px;
  background: #d4f3fd;
  box-shadow: 21px 21px 82px #90a5ac,
    -21px -21px 82px #ffffff;
  font-size: larger;
  padding-left: 80rpx;
  padding-right: 80rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}

.Result {
  width: 60%;
  position: absolute;
  left: 50%;
  top: 30%;
  transform: translate(-50%, -50%);
  /* 50%为自身尺寸的一半 */
  -webkit-transform: translate(-50%, -50%);
  padding: 50rpx;
  border-radius: 40px;
  background: linear-gradient(145deg, #c6dbe6, #ebffff);
  box-shadow: 26px 26px 52px #a5b6bf,
    -26px -26px 52px #ffffff;
  opacity: 0.8;
}

.identifyRes {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 20rpx;
}

.subTitle {
  font-size: large;
  font-weight: bolder;
}

.resTitle {
  font-size: larger;
  font-weight: bolder;
  border-bottom: 2rpx black dotted;
  margin-bottom: 20rpx;
}

.sub{
  width: 100%;
}